<?xml version="1.0" ?>
<robot name="testbot">
	<material name="black">
		<color rgba="0.0 0.0 0.0 1.0"/>
	</material>
	<material name="orange">
		<color rgba="1.0 0.4 0.0 1.0"/>
	</material>
	<link name="base"/>
	<joint name="fixed" type="fixed" output="screen">
		<parent link="base"/>
		<child link="link1"/>
	</joint>
	<link name="link1" output="screen">
		<collision>
			<origin xyz="0 0 0.05" rpy="0 0 0"/>
			<geometry>
				<box size="0.72 0.72 0.1"/>
			</geometry>
		</collision>
		<visual>
			<origin xyz="0 0 0.05" rpy="0 0 0"/>
			<geometry>
				<box size="0.72 0.72 0.1"/>
			</geometry>
			<material name="black"/>
		</visual>
		<inertial>
			<origin xyz="0 0 0.25" rpy="0 0 0"/>
			<mass value="1"/>
			<inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
		</inertial>
	</link>
	<joint name="joint1" type="revolute">
		<parent link="link1"/>
		<child link="link2"/>
		<origin xyz="0 0 0.5" rpy="0 0 0"/>
		<axis xyz="0 0 1"/>
		<limit effort="30" lower="-3.141" upper="3.141" velocity="1.571"/>
	</joint>
	<link name="link2">
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.1 0.1 0.8"/>
			</geometry>
		</collision>
