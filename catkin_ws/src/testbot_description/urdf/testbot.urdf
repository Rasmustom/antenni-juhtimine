			<mass value="1"/>
			<inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
		</inertial>
	</link>
	<joint name="joint3" type="fixed">
		<parent link="link3"/>
		<child link="link4"/>
		<!--<origin xyz="0 0 1.0" rpy="0 0 0"/>-->
		<!--<axis xyz="0 1 0"/>-->
		<!--<limit effort="30" lower="-2.617" upper="2.617" velocity="1.571"/>-->
	</joint>
